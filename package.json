    "@types/jsdom": "^20.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.12",
    "@types/npmlog": "^4.1.4",
    "@types/rimraf": "^3.0.2",
    "@types/sharp": "^0.31.0",
    "@types/shelljs": "^0.8.11",
    "@types/workerpool": "^6.1.0",
    "ansi-colors": "^4.1.3",
    "aws-sdk": "^2.1227.0",
    "babylonjs": "^5.37.0",
